package Giris;
import java.util.Scanner;
public class Baslangic
{
    public static void main(String[] args)
    {
        Scanner inp =  new Scanner(System.in);
        int math, physics, chemistry, english, history, muzik,sum;
        double result;

        System.out.print("Enter the Math note: ");
        math = inp.nextInt();

        System.out.print("Enter the Physics note: ");
        physics = inp.nextInt();

        System.out.print("Enter the Chemistry note: ");
        chemistry = inp.nextInt();

        System.out.print("Enter the Engish note: ");
        english = inp.nextInt();

        System.out.print("Enter the History note: ");
        history = inp.nextInt();

        System.out.print("Enter the Music note: ");
        muzik = inp.nextInt();

        sum = (math + chemistry + english + history + muzik + physics);
        result= sum/6.0;

        String situation = (result> 60) ? "Pasted" : "Failed";
        System.out.print (situation);
    }
}
